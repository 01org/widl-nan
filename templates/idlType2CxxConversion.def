{{

var ConvertIDLType2Cxx = function (idlType) {
  var type = idlType.idlType;
  if (type === 'String' || type === 'DOMString') {
    type = 'std::string';
  } else if (type === 'boolean') {
    type = 'bool';
  } else if (typeof type === 'object' && idlType.generic === 'Promise') {
    type = 'v8::Handle<v8::Promise>';
  }
  return type;
};

var extractArgument = function(idlType) {
  var type = idlType.idlType;
  if (type === 'String' || type === 'DOMString') {
    type = 'string';
  } else if (type === 'boolean') {
    type = 'bool';
  }
  return "EXTRACT_v8_" + type;
};

}}
